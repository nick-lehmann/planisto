{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "es2020",
    "lib": ["es2020"],
    "target": "es2019",
    "paths": {
      "$app/*": [
        ".svelte/dev/runtime/app/*",
        ".svelte/build/runtime/app/*"
      ],
      "$service-worker": [
        ".svelte/build/runtime/service-worker"
      ],
      "$lib/*": [
        "src/lib/*"
      ]
    }
  },
  "include": [
    "src/**/*.d.ts",
    "src/**/*.js",
    "src/**/*.ts",
    "src/**/*.svelte"
  ]
}
